{
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 16,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*js":{
            "when": "$(basename).ts"
        },
        "**/*.js.map": true
    }

}